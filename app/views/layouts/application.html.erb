<!-- START:head -->
<!DOCTYPE html>
<html>
<head>
  <title>Pragprog Books Online Store</title>
<!-- START:stylesheet -->
  <%= stylesheet_link_tag "/stylesheets/scaffold" %>
  <%= stylesheet_link_tag "/stylesheets/depot", :media => "all" %><!-- <label id="code.slt"/> -->
<!-- END:stylesheet -->
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %><!-- <label id="code.csrf"/> -->
</head>
<!-- END:head -->
<body id="store">
	<div id="banner">
		<%= image_tag("logo.png") %>
		<%= @page_title || "Pragmatic Bookshelf" %><!-- <label id="code.depot.e.title"/> -->
	</div>
	<div id="columns">
		<div id="side">
			<a href="http://localhost:3000">Home</a><br />
			<a href="http://www.baidu.com">Questions</a><br />
			<a href="http://www.baidu.com">News</a><br />
			<a href="http://www.baidu.com">Contact</a><br />
		</div>
		<div id="main">
			<%= yield %><!-- <label id="code.depot.e.include"/> -->
		</div>
	</div>
</body>
</html>